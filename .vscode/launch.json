{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter: Run",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "--dart-define-from-file=assets/.env"
      ]
    },
    {
      "name": "Flutter: Profile Mode",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "flutterMode": "profile",
      "args": [
        "--dart-define-from-file=assets/.env"
      ]
    },
    {
      "name": "Flutter: Release Mode",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "flutterMode": "release",
      "args": [
        "--dart-define-from-file=assets/.env"
      ]
    },
    {
      "name": "Flutter: Attach to Device",
      "type": "dart",
      "request": "attach"
    }
  ]
}
